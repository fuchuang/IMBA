  <resultMap id="BaseResultMap" type="com.IMBA.entity.electiveLikes">
    <id column="student_id" jdbcType="INTEGER" property="studentId" />
    <id column="id" jdbcType="INTEGER" property="id" />
    <id column="elective_id" jdbcType="INTEGER" property="electiveId" />
  </resultMap>
