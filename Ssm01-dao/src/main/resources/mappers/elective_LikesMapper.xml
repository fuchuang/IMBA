  </insert>

  <delete id="deleteByPrimaryKey" parameterType="com.IMBA.entity.electiveLikes" >
    delete from stu_elective_likes
    where student_id = #{studentId,jdbcType=INTEGER}
      and elective_id = #{electiveId,jdbcType=INTEGER}
  </delete>

</mapper>